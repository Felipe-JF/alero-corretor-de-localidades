  "version": "0.2.0",
  "configurations": [
    {
      "request": "launch",
      "name": "Launch Program",
      "type": "node",
      "program": "${workspaceFolder}",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--unstable",
        "--inspect-brk",
        "--allow-all",
        "main.ts"
      ],
      "attachSimplePort": 9229,
      "console": "integratedTerminal"
    }
  ]
}
